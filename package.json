{
  "name": "nozzle-js",
  "version": "0.10.0",
  "description": "Chainable stream processing for async iterators. Transform LLM responses, event streams, and more with a fluent API.",
  "keywords": [
    "stream",
    "async",
    "iterator",
    "pipeline",
    "transform",
    "llm",
    "streaming",
    "async-iterator",
    "functional",
    "chainable"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "docs": "typedoc && pnpm run clean-docs",
    "docs:modern": "npx tsx scripts/generate-docs.ts",
    "prepublishOnly": "pnpm run build && pnpm run docs",
    "prerelease": "pnpm run build && pnpm run docs && pnpm run lint && pnpm run test",
    "release:patch": "pnpm run prerelease && npm version patch && npm publish && git push --follow-tags",
    "release:minor": "pnpm run prerelease && npm version minor && npm publish && git push --follow-tags",
    "release:major": "pnpm run prerelease && npm version major && npm publish && git push --follow-tags",
    "release:dry": "pnpm run prerelease && npm pack --dry-run",
    "clean-docs": "find . -name '*.md' -print0 | xargs -0 -I {} perl -i -0777 -pe 's/#### (Type Parameters|Returns)(?:\n((?:(?!^#{2,4} ).)*))?//smg' {}"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "prettier": "3.5.3",
    "puppeteer": "^24.2.1",
    "tsup": "8.5.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "5.8.3",
    "vitest": "3.2.1"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
